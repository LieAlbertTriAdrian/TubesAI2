package percobaan;

import java.util.ArrayList;

public class Main {

	public static void Main(String args[]){
		ArrayList<Integer> A = new ArrayList<Integer>();
		for(int i = 0;i<10;i++){
			Integer Inte = i;
			A.add(Inte);
		}
		
		A.remove(4);
		for(Integer e: A){
			System.out.println(e);
		}
	}
}
